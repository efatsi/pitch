{
  "name": "Pitch",
  "version": "0.0.1",
  "author": "Eli Fatsi",
  "description": "Pitch written in Elixir",
  "dependencies": {
    "underscore": "latest",
    "react": "latest"
  },
  "devDependencies": {
    "reactify": "latest",
    "browserify": "latest",
    "watchify":   "latest"
  },
  "scripts": {
    "build": "browserify -t [ reactify --harmony ] priv/static/js/app/room.js > priv/static/js/bundle.js",
    "watch": "watchify -t [ reactify --harmony ] priv/static/js/app/room.js -o priv/static/js/bundle.js -v"
  }
}
